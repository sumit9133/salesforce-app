                <!-- <apex:pagemessage rendered="{!AND(LIfromoriginatingobj, !isEnterprise)}" summary="You must be on a higher plan to create Invoice using Custom URL Line Items" severity="error" strength="3"></apex:pagemessage>-->
                </apex:outputPanel>
